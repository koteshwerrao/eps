<p id="notice"><%= notice %></p>

<h1>Listing Merchants</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @merchants.each do |m| %>
      <tr>
        <td><%= m.email %></td>
        <td><%#= link_to 'Show', college %></td>
        <td><%#= link_to 'Edit', edit_college_path(college) %></td>
        <td><%#= link_to 'Destroy', college, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

